$ ->
  currentClass = 1

  changeHeaderImage = ->
    classes = ['one', 'two', 'three']
    $('.homepage-top').removeClass($('.homepage-top').data('removeclass'))
    $('.homepage-top').addClass(classes[currentClass])
    $('.homepage-top').data('removeclass', classes[currentClass])
    if currentClass == 2
      currentClass = 0
    else
      currentClass++

  setInterval ->
    changeHeaderImage()
    true
  , 5000

