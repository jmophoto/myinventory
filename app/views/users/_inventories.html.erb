<ul class="list-group">
  <% @user.inspections.each do |i| %>
    <li class="list-group-item">
      <div class="row">
        <div class="col-md-4"><%= i.name %></div>
        <div class="col-md-4"><%= i.display_type %></div>
        <div class="col-md-4">
          <%= link_to "Edit", i, class: "btn btn-primary btn-xs pull-right" %>
        </div>
      </div>
    </li>
  <% end %>
</ul>